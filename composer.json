{
	"name": "labor-digital/typo3-frontend-api",
	"type": "typo3-cms-extension",
	"description": "TYPO3: Provides a frontend API handler with support for Single Page Apps",
	"license": "Apache-2.0",
	"authors": [
		{
			"name": "LABOR.digital",
			"email": "info@labor.tools",
			"homepage": "https://www.labor.digital",
			"role": "Company"
		},
		{
			"name": "Martin Neundorfer",
			"email": "code@neunerlei.eu",
			"homepage": "https://www.neunerlei.eu",
			"role": "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/labor-digital/typo3-frontend-api/issues",
		"source": "https://github.com/labor-digital/typo3-frontend-api",
		"docs": "https://typo3-frontend-api.labor.tools"
	},
	"require": {
		"labor-digital/typo3-better-api": "^10.1.0",
		"league/route": "^5.0.1",
		"0.0.0/composer-include-files": "^1.6",
		"laminas/laminas-httphandlerrunner": "^1.4.0",
		"league/fractal": "^0.18",
		"rosell-dk/webp-convert": "^2.3"
	},
	"require-dev": {
		"roave/security-advisories": "dev-latest"
	},
	"autoload": {
		"psr-4": {
			"LaborDigital\\T3fa\\": "Classes"
		}
	},
	"version": "10.1.3",
	"extra": {
		"t3ba": {
			"isDev": true
		},
		"typo3/cms": {
			"extension-key": "t3fa"
		},
		"include_files": [
			"imagingAutoload.php"
		]
	}
}
